 area = "sortsim"; } call_formCR(area); } function submit_srchby_option(o, srchby_option) { document.getElementById("cafe_option")["srchby"].value = srchby_option; document.getElementById("cafe_option")["nso"].value = get_nso_parameters("rel", "0", "", "", srchby_option); var area = ""; if (srchby_option == "text") { area = "areafieldall"; } else if (srchby_option == "title") { area = "areafieldtit"; } else { area = "areafieldall"; } call_formCR(area); } function submit_dup_option(o, dup_option) { document.getElementById("cafe_option")["dup_remove"].value = dup_option; var area = ""; if (dup_option == 0) { area = "bioinclude"; } else if (dup_option == 1) { area = "bioexclude"; } call_formCR(area); } function submit_board_option(o, board_option) { document.getElementById("cafe_option")["board"].value = board_option; var area = ""; if (board_option == "Q") { area = "boardqna"; } else if (board_option == "T") { area = "boardsales"; } else if (board_option == "K") { area = "boardcafebook"; } else { area = "boardall"; } call_formCR(area); } function get_nso_parameters(so, p, begin_date, end_date, a) { var so_p = ""; var p_p = ""; var a_p = ""; if (so == "rel") { so_p = "r"; } else if (so == "date") { so_p = "dd"; } else { so_p = "r"; } if (p == 0) { p_p = "all"; } else if (p == 1) { p_p = "1d"; } else if (p == 2) { p_p = "1w"; } else if (p == 3) { p_p = "1m"; } else if (p == 4) { p_p = "6m"; } else if (p == 5) { p_p = "1y"; } else if (p == 6) { begin_date = begin_date.replace(/\./gi, ""); end_date = end_date.replace(/\./gi, ""); var base_from = 20031201; var base_to = 20150930; var from = begin_date; var to = end_date; if (eval(from - to) > 0) /* 왼쪽 박스가 미래, 오른쪽 박스가 과거여도 보정 */ { from = end_date; to = begin_date; } if (eval(base_to - from) < 0) /* 시작일에 2222와 같이 기준 종료일 보다 큰 값이 온 경우, 기준 종료일로 */ { from = base_to; to = base_to; } if (eval(base_from - to) > 0) /* 종료일에 1111과 같이 기준 시작일보다 작은 값이 온 경우, 기준 시작일로 */ { from = base_from; to = base_from; } if (eval(base_from - from) > 0) /* 날짜 보정 : 1990.01.01 이전은 1990.01.01로 */ { from = base_from; } if (eval(base_to - to) < 0) /* 현재보다 미래는 오늘날짜로 */ { to = base_to; } p_p = "from" + from + "to" + to; } else { p_p = "all"; } if (a == "text") { a_p = "all"; } else if (a == "title") { a_p = "t"; } else { a_p = "all"; } return "so:" + so_p + ",p:" + p_p + ",a:" + a_p; } function fSearchDateOptionCafe(sDate) { var period; var date_option = 0; var typeofsDate = typeof(sDate); if ( typeofsDate == "string") { period = sDate; } else if ( typeofsDate == "object") { period = sDate.option; } if (period == "all") { document.getElementById("date_option").value = "0"; date_option = 0; } else if (period == "1day") { document.getElementById("date_option").value = "1"; date_option = 1; } else if (period == "1week") { document.getElementById("date_option").value = "2"; date_option = 2; } else if (period == "1month") { document.getElementById("date_option").value = "3"; date_option = 3; } else if (period == "6month") { document.getElementById("date_option").value = "4"; date_option = 4; } else if (period == "1year") { document.getElementById("date_option").value = "5"; date_option = 5; } submit_date_option(date_option, 1); } function submit_date_option(date_option, is_cr) { var area = "periodall"; if (date_option == 0) { document.getElementById("cafe_option")["date_option"].value = 0; document.getElementById("cafe_option")["nso"].value = get_nso_parameters("rel", date_option, "", "", "text"); area = "dateperiodall"; } else if (date_option == 1) { document.getElementById("cafe_option")["date_option"].value = 1; document.getElementById("cafe_option")["nso"].value = get_nso_parameters("rel", date_option, "", "", "text"); area = "dateoneday"; } else if (date_option == 2) { document.getElementById("cafe_option")["date_option"].value = 2; document.getElementById("cafe_option")["nso"].value = get_nso_parameters("rel", date_option, "", "", "text"); area = "dateoneweek"; } else if (date_option == 3) { document.getElementById("cafe